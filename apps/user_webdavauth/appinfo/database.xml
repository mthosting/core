<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>

	<table>

		<name>*dbprefix*webdav_user_mapping</name>

		<declaration>

			<field>
				<name>uid</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>255</length>
			</field>

			<field>
				<name>displayname</name>
				<type>text</type>
				<notnull>false</notnull>
				<length>255</length>
			</field>

			<index>
				<name>webdav_uid</name>
				<unique>true</unique>
				<field>
					<name>uid</name>
				</field>
			</index>

		</declaration>

	</table>

</database>
